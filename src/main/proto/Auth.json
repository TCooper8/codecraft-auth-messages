{
  "namespace": "codecraft.auth",
  "messages": {
    "AddAuth": {
      "email": "string",
      "password": "string"
    },
    "AddAuthReply": {
      "token": "string option",
      "error": "string option"
    },
    "GetAuth": {
      "email": "string",
      "password": "string"
    },
    "GetAuthReply": {
      "token": "string option",
      "error": "string option"
    },
    "ConsumeToken": {
      "token": "string"
    },
    "ConsumeTokenReply": {
      "newToken": "string option",
      "error": "string option"
    }
  },
  "services": {
    "IAuthService": {
      "add": {
        "request": "AddAuth",
        "response": "AddAuthReply"
      },
      "get": {
        "request": "GetAuth",
        "response": "GetAuthReply"
      }
    },
    "ITokenService": {
      "consume": {
        "request": "ConsumeToken",
        "reseponse": "ConsumeTokenReply"
      }
    }
  }
}

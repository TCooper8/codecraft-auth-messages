{
  "namespace": "codecraft.auth",
  "messages": {
    "AddAuth": {
      "email": "string",
      "password": "string"
    },
    "AddAuthReply": {
      "token": "string option",
      "roles": "map[string, string list]",
      "error": "string option"
    },
    "GetAuth": {
      "email": "string",
      "password": "string"
    },
    "GetAuthReply": {
      "token": "string option",
      "roles": "map[string, string list]",
      "error": "string option"
    },
    "ConsumeToken": {
      "token": "string"
    },
    "ConsumeTokenReply": {
      "newToken": "string option",
      "error": "string option"
    },
    "AddRole": {
      "id": "string",
      "token": "string"
    },
    "AddRoleReply": {
      "token": "string option",
      "error": "string option"
    },
    "AddPermission": {
      "id": "string",
      "token": "string",
      "roleId": "string"
    },
    "AddPermissionReply": {
      "token": "string option",
      "error": "string option"
    },
    "GetPermission": {
      "id": "string",
      "token": "string",
      "roleId": "string"
    },
    "GetPermissionReply": {
      "hasPermission": "bool",
      "token": "string option",
      "error": "string option"
    }
  },
  "services": {
    "IAuthService": {
      "add": {
        "request": "AddAuth",
        "response": "AddAuthReply"
      },
      "addRole": {
        "request": "AddRole",
        "response": "AddRoleReply"
      },
      "addPermission": {
        "request": "AddPermission",
        "response": "AddPermissionReply"
      },
      "get": {
        "request": "GetAuth",
        "response": "GetAuthReply"
      },
      "getPermission": {
        "request": "GetPermission",
        "response": "GetPermissionReply"
      },
      "consumeToken": {
        "request": "ConsumeToken",
        "response": "ConsumeTokenReply"
      }
    }
  }
}
